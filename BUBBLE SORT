#include <stdio.h>
  void main()
{
  int a[100], n, i, j,temp,c=0 ;
  c++;
  c++;
  printf("Enter number of elements:\n");
  c++;
  scanf("%d", &n);
  c++;
  printf("Enter the elements:\n");
  c++;
  for (i= 0;i< n;i++)
    scanf("%d", &a[i]);
  for (i=0;i< n - 1;i++)
  {
      c++;
    for (j= 0 ;j< n-i-1;j++)
    {
        c++;
      if (a[j]> a[j+1])
      {
         temp= a[j];
        a[j]= a[j+1];
        a[j+1] =temp;
        c++;
        c++;
        c++;
        c++;
      }}}
  printf("Sorted is :\n");
  for (i = 0;i< n;i++)
  printf("%d\n",a[i]);
  c++;
  c++;
  c++;
  c++;
  printf("\n the space complexity is %d\n",25+(n*4));
  c++;
   c++;
  printf("the time complexity is %d",c+1);
}
